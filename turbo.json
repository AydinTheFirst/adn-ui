{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^build"],
      "persistent": true
    },
    "start": {
      "cache": false,
      "dependsOn": ["^start"],
      "persistent": true
    },
    "//#lint": {},
    "//#format": {},
    "//#type-check": {}
  }
}
